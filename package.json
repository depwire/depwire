{
  "name": "codegraph",
  "version": "0.1.0",
  "description": "Code cross-reference graph builder for TypeScript projects",
  "type": "module",
  "bin": {
    "codegraph": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js"
  },
  "keywords": [
    "typescript",
    "code-analysis",
    "dependency-graph",
    "tree-sitter"
  ],
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.3",
    "graphology": "^0.26.0",
    "graphology-types": "^0.24.8",
    "tree-sitter": "^0.21.1",
    "tree-sitter-typescript": "^0.23.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
